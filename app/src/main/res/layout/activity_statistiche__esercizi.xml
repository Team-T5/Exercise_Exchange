<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Statistiche_esercizi.Statistiche_Esercizi">

    <Button
        android:id="@+id/btnSingolaCategoria"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colore2"
        android:text="@string/strSingleCategory"
        app:layout_constraintBottom_toTopOf="@+id/btnStrutturaEsame"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnStrutturaEsame"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colore3"
        android:text="@string/strExamStructure"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSingolaCategoria" />
</androidx.constraintlayout.widget.ConstraintLayout>